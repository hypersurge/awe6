package org.flashdevelop.utils;

/**
* Predefined trace level values for the tracing classes.
* @author Mika Palmu
* @author Franco Ponticelli (haXe version)
* @version 1.0
*/
class TraceLevel {
   /**
   * Constants of the class
   */
   public static inline var INFO = 0;
   public static inline var DEBUG = 1;
   public static inline var WARNING = 2;
   public static inline var ERROR = 3;
   public static inline var FATAL = 4;
}